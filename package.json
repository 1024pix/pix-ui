{
  "name": "@1024pix/pix-ui",
  "version": "13.4.4",
  "description": "Pix-UI is the implementation of Pix design principles and guidelines for its products.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "GIP Pix",
  "engines": {
    "node": "^16.13.0"
  },
  "ember": {
    "edition": "octane"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1024pix/pix-ui.git"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "./scripts/build.sh && npm run signal-github",
    "build-ember": "ember build --environment=production",
    "build-storybook": "ember build && cp -v CNAME dist && build-storybook -s dist",
    "clean": "rm -rf dist node_modules",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "npm run lint:js && npm run lint:hbs",
    "lint:fix": "npm run lint:js:fix && npm run lint:hbs:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "npm run lint:hbs -- --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "start": "ember serve",
    "storybook": "ember build && ember serve & start-storybook -p 9001 -s dist",
    "test": "ember test",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "signal-github": "./scripts/signal-deploy-to-pr.sh"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.6.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-string-utils": "^1.1.0",
    "ember-click-outside": "^2.0.0",
    "ember-prop-modifier": "^1.0.1",
    "ember-truth-helpers": "^3.0.0"
  },
  "devDependencies": {
    "@1024pix/ember-testing-library": "^0.5.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.4",
    "@ember/test-helpers": "^2.6.0",
    "@formatjs/intl": "^2.1.0",
    "@fortawesome/ember-fontawesome": "^0.2.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-centered": "^5.3.21",
    "@storybook/addon-controls": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/ember": "^6.3.7",
    "@storybook/ember-cli-storybook": "^0.4.0",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/theming": "^6.3.7",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-ember-modules-api-polyfill": "^2.13.4",
    "babel-plugin-htmlbars-inline-precompile": "^3.2.0",
    "broccoli-asset-rev": "^3.0.0",
    "core-js": "^3.16.1",
    "ember-auto-import": "^1.12.0",
    "ember-cli": "^3.27.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^5.0.0",
    "ember-source": "^3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-template-lint-plugin-prettier": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^6.2.0",
    "fs-extra": "^10.0.0",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "qunit-dom": "^1.6.0",
    "sass": "^1.37.5"
  },
  "bugs": {
    "url": "https://github.com/1024pix/pix-ui/issues"
  },
  "homepage": "https://github.com/1024pix/pix-ui#readme",
  "main": "index.js",
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "storybook-deployer": {
    "commitMessage": "Deploy Storybook [skip ci]"
  }
}
