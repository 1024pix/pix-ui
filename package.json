{
  "name": "@1024pix/pix-ui",
  "version": "34.1.0",
  "description": "Pix-UI is the implementation of Pix design principles and guidelines for its products.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "GIP Pix",
  "engines": {
    "node": "16",
    "npm": ">=8.13.2 <9"
  },
  "ember": {
    "edition": "octane"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1024pix/pix-ui.git"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "build-ember": "ember build --environment=production",
    "serve-ember": "ember serve",
    "build-storybook": "ember build && cp -v CNAME dist && build-storybook",
    "serve-storybook": "start-storybook --port 9001",
    "clean": "rm -rf dist node_modules",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "npm-run-all --aggregate-output --parallel --continue-on-error 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --parallel --continue-on-error lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "npm run lint:hbs -- --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint app/styles/*.scss addon/styles/*.scss 'addon/styles/**/*.scss'",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "preinstall": "npx check-engine",
    "start": "ember serve",
    "prestorybook": "ember build",
    "storybook": "npm-run-all --parallel serve-ember serve-storybook",
    "test": "ember test",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@formatjs/intl": "^2.5.1",
    "ember-auto-import": "^2.5.0",
    "ember-cli-babel": "^7.26.8",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-sass": "^11.0.1",
    "ember-click-outside": "^6.0.1",
    "ember-popperjs": "^3.0.0",
    "ember-truth-helpers": "^3.1.1",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@1024pix/ember-testing-library": "^0.6.0",
    "@1024pix/stylelint-config": "^1.0.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.5",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^2.1.1",
    "@fortawesome/ember-fontawesome": "^1.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-controls": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addons": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/ember": "^6.5.13",
    "@storybook/ember-cli-storybook": "^0.6.0",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/theming": "^6.5.13",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/user-event": "^14.4.3",
    "chromatic": "^6.17.3",
    "core-js": "^3.26.1",
    "ember-cli": "^4.8.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^10.0.0",
    "ember-sinon": "^5.0.0",
    "ember-source": "^4.0.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.18.2",
    "ember-template-lint-plugin-prettier": "^4.1.0",
    "ember-try": "^3.0.0-beta",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.3",
    "fs-extra": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "sass": "^1.56.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^6.1.0",
    "webpack": "^5.75.0"
  },
  "overrides": {
    "@storybook/ember": {
      "ember-source": "^4.0.1"
    }
  },
  "bugs": {
    "url": "https://github.com/1024pix/pix-ui/issues"
  },
  "homepage": "https://github.com/1024pix/pix-ui#readme",
  "main": "index.js",
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "storybook-deployer": {
    "commitMessage": "Deploy Storybook [skip ci]"
  }
}
